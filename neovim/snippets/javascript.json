{
  "const": {
    "prefix": "c",
    "body": "const ${1:fName} = $2"
  },
  "const function": {
    "prefix": "cf",
    "body": "const ${1:fName} = ($2) => {\n  $0\n};"
  },
  "const function export default": {
    "prefix": "ce",
    "body": "const ${1:fName} = ($2) => {\n  $0\n};\n\nexport default ${1};"
  },
  "custom react tag": {
    "prefix": "rt",
    "body": "<$1>$CLIPBOARD $0</$1>"
  },
  "export { default as }": {
    "prefix": "ea",
    "body": "export { default as ${1:pName} } from \"./${1}\";"
  },
  "export default": {
    "prefix": "ed",
    "body": "export default ${1:name};"
  },
  "export type": {
    "prefix": "et",
    "body": "export type ${1:name};"
  },
  "export interface": {
    "prefix": "ei",
    "body": "export interface ${1:name};"
  },
  "if statement": {
    "prefix": "if",
    "body": "if ($1) {\n$CLIPBOARD\n$0\n}"
  },
  "import from": {
    "prefix": "im",
    "body": "import $1 from \"${2}\"${3:;}"
  },
  "export const fn": {
    "prefix": "ec",
    "body": "export const $1 = $2;"
  },
  "console log": {
    "prefix": "cl",
    "body": "console.log($1, $0);"
  },
  "full describe()": {
    "prefix": "ds",
    "body": "describe(\"${1:name}\", () => {\n$0\n});"
  },
  "full it()": {
    "prefix": "it",
    "body": "it(\"${1:name}\", () => {\n$0\n});"
  },
  "Object.keys()": {
    "prefix": "Ok",
    "body": "Object.keys($1)"
  },
  "Object.values()": {
    "prefix": "Ov",
    "body": "Object.values($1)"
  },
  "Object.entries()": {
    "prefix": "Oe",
    "body": "Object.entries($1)"
  },
  "Array.isArray()": {
    "prefix": "Ais",
    "body": "Array.isArray($1)"
  },
  "Array.from()": {
    "prefix": "Afr",
    "body": "Array.from($1)"
  },
  "Reflect.has()": {
    "prefix": "Rh",
    "body": "Reflect.has($1)"
  },
  "Reflect.get()": {
    "prefix": "Rg",
    "body": "Reflect.get($1)"
  },
  "Reflect.set()": {
    "prefix": "Rs",
    "body": "Reflect.set($1)"
  },
  "Reflect.delete()": {
    "prefix": "Rd",
    "body": "Reflect.delete($1)"
  }
}
