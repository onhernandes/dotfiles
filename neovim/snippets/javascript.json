{
  "const": {
    "prefix": "cct",
    "body": "const ${1} = $2"
  },
  "const function": {
    "prefix": "cf",
    "body": "const ${1} = ($2) => {\n  $0\n};"
  },
  "export const function": {
    "prefix": "ecf",
    "body": "export const ${1} = ($2) => {\n  $0\n};"
  },
  "const function export default": {
    "prefix": "ce",
    "body": "const ${1} = ($2) => {\n  $0\n};\n\nexport default ${1};"
  },
  "try catch": {
    "prefix": "tc",
    "body": "try {\n$CLIPBOARD$2\n} catch (error) {\n  console.log(error)$1\n}"
  },
  "eslint-disable-next-line": {
    "prefix": "esd",
    "body": "// eslint-disable-next-line $1"
  },
  "export { default as }": {
    "prefix": "eas",
    "body": "export { default as ${1} } from \"./${1}\";"
  },
  "export * as": {
    "prefix": "eall",
    "body": "export * as ${1} from \"./${1}\";"
  },
  "export default": {
    "prefix": "ed",
    "body": "export default ${1};"
  },
  "if statement": {
    "prefix": "if",
    "body": "if ($1) {\n$CLIPBOARD$0\n}"
  },
  "import from": {
    "prefix": "im",
    "body": "import $1 from \"${2}\"${3:;}"
  },
  "export const fn": {
    "prefix": "ec",
    "body": "export const $1 = $2;"
  },
  "console log": {
    "prefix": "cl",
    "body": "console.log($1, $0);"
  },
  "full describe()": {
    "prefix": "ds",
    "body": "describe(\"${1}\", () => {\n$0\n});"
  },
  "full it()": {
    "prefix": "it",
    "body": "it(\"${1}\", () => {\n$0\n});"
  },
  "Object.keys()": {
    "prefix": "Ok",
    "body": "Object.keys($1)"
  },
  "Object.values()": {
    "prefix": "Ov",
    "body": "Object.values($1)"
  },
  "Object.entries()": {
    "prefix": "Oe",
    "body": "Object.entries($1)"
  },
  "Array.isArray()": {
    "prefix": "Ais",
    "body": "Array.isArray($1)"
  },
  "Array.from()": {
    "prefix": "Afr",
    "body": "Array.from($1)"
  },
  "Reflect.has()": {
    "prefix": "Rh",
    "body": "Reflect.has($1)"
  },
  "Reflect.get()": {
    "prefix": "Rg",
    "body": "Reflect.get($1)"
  },
  "Reflect.set()": {
    "prefix": "Rs",
    "body": "Reflect.set($1)"
  },
  "Reflect.delete()": {
    "prefix": "Rd",
    "body": "Reflect.delete($1)"
  }
}
